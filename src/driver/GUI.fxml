<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="driver.Controller">
   <children>
      <VBox layoutX="100.0">
         <children>
            <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="282.0" prefWidth="600.0" spacing="20.0" AnchorPane.leftAnchor="100.0">
               <children>
                  <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#addGame" text="+" />
                  <Button fx:id="subtractButton" mnemonicParsing="false" onAction="#subtractGame" text="-" />
                  <Label fx:id="gamesLabel" text="0 Games played" />
                  <AnchorPane prefHeight="200.0" prefWidth="200.0" />
               </children>
            </HBox>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button alignment="CENTER" layoutX="202.0" layoutY="68.0" mnemonicParsing="false" onAction="#clearGames" prefHeight="65.0" prefWidth="98.0" text="Clear Games" textFill="#270fbf" />
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
